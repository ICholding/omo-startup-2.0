NODE_ENV=development
PORT=3001

# Comma-separated list of allowed frontend origins for CORS.
# Leave empty to allow all origins.
CORS_ALLOWED_ORIGINS=http://localhost:5173

ACTIVE_AGENT_ID=chat-mode
CLIENT_NAME=Default Client
INDUSTRY=General
FEATURE_FILE_UPLOAD=true
FEATURE_PAPERCLIP_VISIBLE=true
FEATURE_INTAKE_QUESTIONS=true
BRAND_PRIMARY_COLOR=#3B82F6
BRAND_LOGO_URL=
BRAND_COMPANY_NAME=

ADMIN_ENABLED=true
ADMIN_USERNAME=admin
ADMIN_PASSWORD=changeme123
CUSTOMIZATION_STORAGE=file

# Optional OpenRouter + GPT integration for advanced chat responses
OPENROUTER_API_KEY=
OPENROUTER_MODEL=openai/gpt-4o-mini
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_SITE_URL=http://localhost:5173
OPENROUTER_APP_NAME=omo-startup-chat
OPENROUTER_TEMPERATURE=0.4
OPENROUTER_MAX_CONTEXT_MESSAGES=12

# Controls backend "interleaved thinking" pacing
THINKING_DELAY_MS=250
THINKING_DELAY_MAX_MS=2200
