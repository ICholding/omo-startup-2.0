NODE_ENV=development
PORT=3001
CORS_ALLOWED_ORIGINS=http://localhost:5173

AGENT_PROVIDER=moltbot
MOLTBOT_URL=http://localhost:8080
OPENCLAW_GATEWAY_URL=http://openclaw-gateway:18789
MOLTBOT_CHAT_PATH=/api/chat/message
MOLTBOT_HEALTH_PATH=/health

CLIENT_NAME=Default Client
INDUSTRY=General

# Telegram Bot Integration
# Get token from @BotFather on Telegram (much faster than WhatsApp Business API)
TELEGRAM_BOT_TOKEN=your_bot_token_from_botfather
TELEGRAM_WEBHOOK_SECRET=random_secret_for_webhook_security
TELEGRAM_ALLOWED_CHAT_IDS=optional_chat_id_filter_comma_separated

# Clawbot API Configuration
CLAWBOT_API_URL=http://localhost:8080
CLAWBOT_API_KEY=optional_api_key
CLAWBOT_TIMEOUT=300000
