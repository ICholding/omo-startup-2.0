# OpenClaw Agent Configuration
# Copy to .env and fill in your values

# Server
PORT=8080
NODE_ENV=production

# OpenRouter Configuration (Required)
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_API_URL=https://openrouter.ai/api/v1
OPENROUTER_MODEL=claude-3.5-sonnet

# Agent Identity
AGENT_ID=openclaw-agent

# Optional: API Key for client authentication
# If set, clients must provide this in Authorization header
API_KEY=your_secret_api_key_here

# Backend URL (for CORS/referer)
OMO_BACKEND_URL=https://omo-startup-backend.onrender.com

# Optional: GitHub Integration
GITHUB_TOKEN=your_github_token_here
GITHUB_OWNER=ICholding
GITHUB_REPO=omo-startup-2.0

# Optional: Backblaze B2
B2_APPLICATION_KEY_ID=your_b2_key_id
B2_APPLICATION_KEY=your_b2_key
B2_BUCKET_NAME=omo-LLM
